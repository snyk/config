{
  "name": "snyk-config",
  "description": "Config setup for snyk shared across projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prepare": "npm run build",
    "lint": "eslint 'lib/**/*.?s'",
    "test": "jest",
    "format": "prettier --write '{lib,test}/**/*.?s'"
  },
  "keywords": [
    "config"
  ],
  "author": "Remy Sharp",
  "license": "(Apache-2.0 AND MIT)",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^6.14.9",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "prettier": "^1.19.1",
    "ts-jest": "^26.4.4",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "async": "^3.2.0",
    "debug": "^4.1.1",
    "lodash.merge": "^4.6.2",
    "minimist": "^1.2.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/config.git"
  }
}
